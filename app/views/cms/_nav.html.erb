<%= cms_tag_list(:ul, @obj.homepage, :toclist, class: "nav navbar-nav", id: "main_nav") do |list, item| %>
  <% list.tag(:li, id: "main_nav_page_#{item.id}", data: { path: item.path }) do %>
    <%= link_to(cms_path(item)) do %>
      <%= display_field(item, :title) %>
    <% end %>
  <% end %>
<% end %>
