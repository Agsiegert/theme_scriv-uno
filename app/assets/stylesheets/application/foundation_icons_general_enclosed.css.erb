/* font-face */
@font-face {
  font-family: "foundation_icons_general_enclosed";
  src: url("<%= asset_path 'foundation_icons_general_enclosed.eot' %>");
  src: url("<%= asset_path 'foundation_icons_general_enclosed.woff' %>") format("woff"), url("<%= asset_path 'foundation_icons_general_enclosed.ttf' %>") format("truetype");
  font-weight: normal;
  font-style: normal;
}

/* global foundicon styles */
[class*="foundicon-"] {
  font-size:20px;
  padding:0 5px 0 0;
  display: inline-block;
  width: auto;
  height: auto;
  line-height: inherit;
  vertical-align: middle;
  background-image: none;
  background-position: 0 0;
  background-repeat: repeat;
}

